{
  "hosting": {
    "public": "web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "redirects": [ {
      "source": "/discord",
      "destination": "/discord.html",
      "type": 301
    } ],
    "redirects": [ {
      "source": "/1",
      "destination": "/votesite1.html",
      "type": 301
    } ],
    "redirects": [ {
      "source": "/2",
      "destination": "/votesite2.html",
      "type": 301
    } ],
    "redirects": [ {
      "source": "/3",
      "destination": "/votesite3.html",
      "type": 301
    } ],
    "redirects": [ {
      "source": "/4",
      "destination": "/votesite4.html",
      "type": 301
    } ],
    "redirects": [ {
      "source": "/5",
      "destination": "/votesite5.html",
      "type": 301
    } ]
  }
}
