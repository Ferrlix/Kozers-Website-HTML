{
  "hosting": {
    "public": "web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ 
      {
        "source": "/discord",
        "destination": "/discord.html",
        "type": 301
      },
      {
        "source": "/votes/1",
        "destination": "/votes/votesite1.html",
        "type": 301
      },
      {
        "source": "/votes/2",
        "destination": "/votes/votesite2.html",
        "type": 301
      },
      {
        "source": "/votes/3",
        "destination": "/votes/votesite3.html",
        "type": 301
      },
      {
        "source": "/votes/4",
        "destination": "/votes/votesite4.html",
        "type": 301
      },
      {
        "source": "/votes/5",
        "destination": "/votes/votesite5.html",
        "type": 301
      } 
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
